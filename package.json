{
  "name": "we-edit",
  "description": "an text editor",
  "version": "0.0.1",
  "author": {
    "name": "Raymond Li",
    "email": "lalalic@139.com"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "karma": ">=0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
	"uglifyify": "^3.0.1",
    "watchify": "^3.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "babel-plugin-transform-object-rest-spread",
	  "babel-plugin-transform-class-properties"
    ]
  },
  "scripts":{
	"build": "babel src -d lib --source-maps inline",
    "dist": "browserify -g uglifyify -r ./lib/index.js:we-edit -o dist/index.js",
	"dist.watch": "watchify -v -d -r ./lib/index.js:we-edit -o dist/index.js",

	"karma": "karma start",
    "test": "http-server dist"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">= 0.4.4"
  },
  "readme": "README.md"
}
